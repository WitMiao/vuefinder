{
    "extends": "@vue/tsconfig/tsconfig.dom.json",
    "include": ["./src/**/*"],
    "compilerOptions": {
        "moduleResolution": "bundler",
        "module": "ESNext",
        "target": "ES2020",
        "allowJs": true,
        "checkJs": true,
        "declaration": true,
        "declarationMap": true,
        "emitDeclarationOnly": true,
        "outDir": "dist",
        "strict": false,
        "skipLibCheck": true,
    }
}